{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es6",
    "noImplicitReturns": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "lib": [
      "es2017",
      "dom"
    ],
  }
}
